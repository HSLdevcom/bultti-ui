  "scripts": {
    "start": "BROWSER=none EXTEND_ESLINT=true PORT=3001 react-app-rewired start",
    "start:build": "PORT=3001 node -r dotenv/config serve.js",
    "build": "EXTEND_ESLINT=true react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "codegen": "graphql-codegen --config codegen.yml",
    "codestyle": "prettier \"src/**/*.{ts,js,json}\" --write",
    "postinstall": "patch-package"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "react-hooks/exhaustive-deps": "off",
      "no-unexpected-multiline": "off",
      "no-restricted-globals": "off",
      "no-var": "off",
      "prefer-const": "off",
      "no-redeclare": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "^4.1.1",
    "**/@typescript-eslint/parser": "^4.1.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.17.8",
    "@graphql-codegen/fragment-matcher": "^1.17.8",
